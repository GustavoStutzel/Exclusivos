

****EXPORTA VENDAS ABRAND****************************

TEXT TO XSEL NOSHOW TEXTMERGE

declare @data_ini datetime
declare @data_fim datetime
select @data_ini='20150701'
select @data_fim='20150731'

select D.CODIGO_FILIAL,C.FILIAL, C.SUPERVISORA, A.VENDEDOR,D.NOME_VENDEDOR,E.TOTAL_VENDAS,A.TOTAL_NOVOS+ISNULL(B.TOT_SEM_CPF,0) AS TOTAL_NOVOS,ISNULL(B.TOT_SEM_CPF,0) AS TOTAL_SEM_CPF,
ISNULL(F.TOT_INCOMP,0) AS TOTAL_INCOMPLETO,
ISNULL(convert(numeric(14,2),((convert(numeric(14,2),B.TOT_SEM_CPF)/convert(numeric(14,2),A.TOTAL_NOVOS+ISNULL(B.TOT_SEM_CPF,0)))*100)),0) AS PORCENTAGEM
FROM
(SELECT A.VENDEDOR,COUNT(A.TICKET) AS TOTAL_NOVOS 
FROM wanm_vendas_loja_full A
JOIN CLIENTES_VAREJO B
ON A.CODIGO_CLIENTE = B.CODIGO_CLIENTE
WHERE A.DATA_VENDA BETWEEN @data_ini AND @data_fim
AND B.CADASTRAMENTO BETWEEN @data_ini AND @data_fim
AND A.VALOR_PAGO > '0'
GROUP BY A.VENDEDOR) A

LEFT JOIN
(SELECT VENDEDOR,COUNT(TICKET) AS TOT_SEM_CPF 
FROM wanm_vendas_loja_full
WHERE CODIGO_CLIENTE IN ('77777777777','12345678909')
AND DATA_VENDA BETWEEN @data_ini AND @data_fim
AND VALOR_PAGO > '0'
GROUP BY VENDEDOR) B
ON A.VENDEDOR = B.VENDEDOR

LEFT JOIN
(SELECT A.VENDEDOR,COUNT(A.TICKET) AS TOT_INCOMP
FROM wanm_vendas_loja_full A
JOIN CLIENTES_VAREJO B
ON A.CODIGO_CLIENTE = B.CODIGO_CLIENTE
WHERE (B.ENDERECO = '' OR B.CIDADE ='' OR B.CEP ='' OR B.EMAIL='' OR B.BAIRRO =''
OR B.CELULAR ='' OR B.NUMERO ='' OR B.UF = '' or B.ENDERECO = 'CADASTRO INCOMPLETO') 
AND B.CADASTRAMENTO BETWEEN @data_ini AND @data_fim
AND A.DATA_VENDA BETWEEN @data_ini AND @data_fim
GROUP BY A.VENDEDOR) F
ON A.VENDEDOR = F.VENDEDOR

LEFT JOIN 
(SELECT VENDEDOR,COUNT(TICKET) AS TOTAL_VENDAS
FROM wanm_vendas_loja_full
WHERE DATA_VENDA BETWEEN @data_ini AND @data_fim
AND VALOR_PAGO > '0' GROUP BY VENDEDOR) E
ON A.VENDEDOR = E.VENDEDOR 

JOIN LOJA_VENDEDORES D
ON A.VENDEDOR = D.VENDEDOR

JOIN
WANM_LOJAS_VAREJO C
ON D.CODIGO_FILIAL = C.CODIGO_FILIAL

ORDER BY CODIGO_FILIAL 


ENDTEXT 

