

****EXPORTA SMS ABRAND****************************

TEXT TO XSEL NOSHOW TEXTMERGE

SELECT B.FILIAL AS 'FILIAL DE ENTRADA',B.ORDEM_PRODUCAO,A.PRODUTO,C.DESC_PRODUTO,A.QTDE_TOTAL, C.GRIFFE,C.COLECAO, 
CONVERT (varchar,B.EMISSAO,104) AS 'DATA ENTRADA' 
FROM PRODUCAO_ORDEM A
JOIN ESTOQUE_PROD_ENT B
ON A.ORDEM_PRODUCAO = B.ORDEM_PRODUCAO
JOIN PRODUTOS C
ON A.PRODUTO = C.PRODUTO
WHERE B.FILIAL='ATACADO' 
AND B.ORDEM_PRODUCAO IS NOT NULL
AND C.GRIFFE IN ('EMILLY','LUISA')
AND C.COLECAO = 'VER11'
ORDER BY C.GRIFFE,B.EMISSAO

ENDTEXT 

F_SELECT(XSEL,"CUR_EXPORTA")

SELE CUR_EXPORTA
COPY TO "G:\Amanda.Magalhaes\op_encerrada_atacado.XLS" XLS 