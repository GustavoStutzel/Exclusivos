

****EXPORTA SMS ABRAND****************************

TEXT TO XSEL NOSHOW TEXTMERGE

SELECT DISTINCT B.NOME_CLIFOR,B.RAZAO_SOCIAL,B.ENDERECO,B.NUMERO,B.COMPLEMENTO,B.BAIRRO,B.CIDADE,B.UF,
B.DDD1,B.TELEFONE1,B.TELEFONE2,B.EMAIL 
FROM CADASTRO_CLI_FOR B
JOIN CLIENTES_ATACADO C
ON B.NOME_CLIFOR = C.CLIENTE_ATACADO
AND B.CLIFOR = C.COD_CLIENTE
JOIN MATERIAIS_2_QUALI D
ON C.COD_CLIENTE = D.COD_MAQUINA
WHERE UF = 'SP' AND C.TIPO_BLOQUEIO='LIBERADO' AND C.TIPO != 'LOJAS'

ENDTEXT 

F_SELECT(XSEL,"CUR_EXPORTA")

SELE CUR_EXPORTA
COPY TO "C:\TEMP\CLIENTE_VIP_SP.XLS" XLS 