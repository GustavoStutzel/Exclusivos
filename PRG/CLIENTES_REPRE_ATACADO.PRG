

****EXPORTA SMS ABRAND****************************

TEXT TO XSEL NOSHOW TEXTMERGE

SELECT DISTINCT B.NOME_CLIFOR,B.RAZAO_SOCIAL,B.ENDERECO,B.NUMERO,B.COMPLEMENTO,B.BAIRRO,B.CIDADE,B.UF,
B.DDD1,B.TELEFONE1,B.TELEFONE2,B.EMAIL 
FROM VENDAS A
JOIN CADASTRO_CLI_FOR B
ON A.CLIENTE_ATACADO = B.NOME_CLIFOR
JOIN CLIENTES_ATACADO C
ON A.CLIENTE_ATACADO = C.CLIENTE_ATACADO
WHERE A.REPRESENTANTE = 'Mônica Altoé' AND A.COLECAO='INV11' AND C.TIPO_BLOQUEIO='LIBERADO'
ENDTEXT 

F_SELECT(XSEL,"CUR_EXPORTA")

SELE CUR_EXPORTA
COPY TO "P:\Rafaela\Clientes_Monica Altoe.XLS" XLS 