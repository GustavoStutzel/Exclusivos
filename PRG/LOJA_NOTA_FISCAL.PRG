TEXT TO XSEL NOSHOW TEXTMERGE

SELECT A.CODIGO_FILIAL AS COD, D.FILIAL, A.NF_C_PULO, 
ISNULL(B.NF_C_ERRO,0) AS NF_C_ERRO, 
ISNULL(C.NF_S_ENVIAR,0) AS NF_S_ENVIAR
FROM
(SELECT CODIGO_FILIAL, (max(NF_NUMERO)- count (NF_NUMERO)) AS NF_C_PULO
FROM LOJA_NOTA_FISCAL WHERE NF_NUMERO != '014781'
GROUP BY CODIGO_FILIAL) A
LEFT JOIN
(SELECT CODIGO_FILIAL, COUNT(NF_NUMERO) AS NF_C_ERRO
FROM LOJA_NOTA_FISCAL 
WHERE STATUS_NFE NOT IN ('5','49','59','1') 
GROUP BY CODIGO_FILIAL) B
ON A.CODIGO_FILIAL = B.CODIGO_FILIAL
LEFT JOIN
(SELECT CODIGO_FILIAL, COUNT(NF_NUMERO) AS NF_S_ENVIAR
FROM LOJA_NOTA_FISCAL 
WHERE STATUS_NFE = '1'
GROUP BY CODIGO_FILIAL ) C
ON A.CODIGO_FILIAL = C.CODIGO_FILIAL
JOIN LOJAS_VAREJO D
ON A.CODIGO_FILIAL = D.CODIGO_FILIAL

ENDTEXT 

F_SELECT(XSEL,"CUR_EXPORTA")

SELE CUR_EXPORTA
COPY TO C:\TEMP\LOJA_NOTA_FISCAL.XLS XLS 