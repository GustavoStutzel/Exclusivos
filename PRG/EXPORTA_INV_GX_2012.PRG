SELECT C.MATRIZ,D.CGC_CPF AS CNPJ_MATRIZ,A.FILIAL,E.CGC_CPF AS CNPJ_FILIAL,
A.MATERIAL,B.DESC_MATERIAL,B.CLASSIF_FISCAL,F.DESC_CLASSIFICACAO,
CONVERT(NUMERIC(14,2),SUM(A.QTDE)) AS QTDE,
CONVERT(NUMERIC(14,2),SUM(G.CUSTO_REPOSICAO*A.QTDE )) AS VALOR
FROM TMPSID_ESTOQUE_MAT_31122012 A  
JOIN MATERIAIS B 
ON A.MATERIAL=B.MATERIAL 
JOIN FILIAIS C 
ON A.FILIAL=C.FILIAL 
JOIN CADASTRO_CLI_FOR D 
ON C.MATRIZ=D.NOME_CLIFOR 
JOIN CADASTRO_CLI_FOR E 
ON A.FILIAL=E.NOME_CLIFOR 
JOIN CLASSIF_FISCAL F 
ON B.CLASSIF_FISCAL=F.CLASSIF_FISCAL 
JOIN MATERIAIS_CORES G 
ON A.MATERIAL =G.MATERIAL 
AND A.COR_MATERIAL =G.COR_MATERIAL 
WHERE QTDE<>0
GROUP BY C.MATRIZ,D.CGC_CPF,A.FILIAL,E.CGC_CPF ,
A.MATERIAL,B.DESC_MATERIAL,B.CLASSIF_FISCAL,F.DESC_CLASSIFICACAO  
order by C.MATRIZ,A.FILIAL,A.MATERIAL


endtext 

