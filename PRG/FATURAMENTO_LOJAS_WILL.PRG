select B.CODIGO_FILIAL,A.NOME_CLIFOR AS FILIAL,C.ROMANEIO,C.CAIXA,C.PRODUTO,C.QTDE,A.data_embalagem AS DATA 
from FATURAMENTO_CAIXAS A
JOIN LOJAS_VAREJO B
ON A.NOME_CLIFOR = B.FILIAL
JOIN FATURAMENTO_PROD C
ON A.CAIXA = C.CAIXA
where A.data_embalagem between '20101116' and '20101125'
ORDER BY B.CODIGO_FILIAL

