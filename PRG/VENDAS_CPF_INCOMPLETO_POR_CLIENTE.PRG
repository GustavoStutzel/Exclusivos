

****EXPORTA VENDAS ABRAND****************************

TEXT TO XSEL NOSHOW TEXTMERGE

declare @data_ini datetime
declare @data_fim datetime
select @data_ini='20110401'
select @data_fim='20110412'

select DISTINCT A.CODIGO_FILIAL,C.FILIAL,A.VENDEDOR,A.CODIGO_CLIENTE,A.CLIENTE_VAREJO,D.NOME_VENDEDOR
FROM
(SELECT A.CODIGO_FILIAL,A.CODIGO_CLIENTE,B.CLIENTE_VAREJO,A.VENDEDOR,COUNT(A.TICKET) AS TOT_INCOMP
FROM wanm_vendas_loja_full A
JOIN CLIENTES_VAREJO B
ON A.CODIGO_CLIENTE = B.CODIGO_CLIENTE
WHERE (B.ENDERECO = '' OR B.CIDADE ='' OR B.CEP ='' OR B.EMAIL='' OR B.BAIRRO =''
OR B.CELULAR ='' OR B.NUMERO ='' OR B.UF = '') 
AND B.CADASTRAMENTO BETWEEN @data_ini AND @data_fim
AND A.DATA_VENDA BETWEEN @data_ini AND @data_fim
GROUP BY A.CODIGO_FILIAL,A.CODIGO_CLIENTE,B.CLIENTE_VAREJO,A.VENDEDOR) A

JOIN
LOJAS_VAREJO C
ON A.CODIGO_FILIAL = C.CODIGO_FILIAL

JOIN LOJA_VENDEDORES D
ON A.VENDEDOR = D.VENDEDOR
ORDER BY CODIGO_FILIAL,NOME_VENDEDOR 


ENDTEXT 

