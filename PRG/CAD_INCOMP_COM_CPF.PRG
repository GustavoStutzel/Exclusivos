

****EXPORTA VENDAS ABRAND****************************

TEXT TO XSEL NOSHOW TEXTMERGE

declare @data_ini datetime
declare @data_fim datetime
select @data_ini='20120201'
select @data_fim='20120229'

SELECT A.CODIGO_FILIAL,A.VENDEDOR,C.NOME_VENDEDOR,A.CODIGO_CLIENTE,B.CLIENTE_VAREJO
FROM wanm_vendas_loja_full A
JOIN CLIENTES_VAREJO B
ON A.CODIGO_CLIENTE = B.CODIGO_CLIENTE
JOIN LOJA_VENDEDORES C
ON A.VENDEDOR = C.VENDEDOR
WHERE (B.ENDERECO = '' OR B.ENDERECO = 'CADASTRO INCOMPLETO' OR B.CIDADE ='' OR B.CEP ='' OR B.EMAIL='' OR B.BAIRRO =''
OR B.CELULAR ='' OR B.NUMERO ='' OR B.UF = '') 
AND B.CADASTRAMENTO BETWEEN @data_ini AND @data_fim
AND A.DATA_VENDA BETWEEN @data_ini AND @data_fim
ORDER BY A.CODIGO_FILIAL


ENDTEXT 

F_SELECT(XSEL,"CUR_EXPORTA")

SELE CUR_EXPORTA
