TEXT TO XSEL NOSHOW TEXTMERGE

SELECT B.MATRIZ,G.CGC_CPF AS CNPJ_MATRIZ,A.FILIAL,H.CGC_CPF AS CNPJ_FILIAL,
A.PRODUTO,C.DESC_PRODUTO,C.CLASSIF_FISCAL,
A.SALDO,D.PRECO1 AS PRECO_CUSTO_GERENCIAL,E.PRECO1 AS PRECO_VAREJO_ORIGINAL 
FROM TMP_ESTOQUES_31122012_2 A 
JOIN FILIAIS B 
ON A.FILIAL=B.FILIAL 
JOIN PRODUTOS C 
ON A.PRODUTO=C.PRODUTO
LEFT JOIN (SELECT PRODUTO,PRECO1 FROM PRODUTOS_PRECOS WHERE CODIGO_TAB_PRECO='CT')  D 
ON A.PRODUTO=D.PRODUTO
LEFT JOIN (SELECT PRODUTO,PRECO1 FROM PRODUTOS_PRECOS WHERE CODIGO_TAB_PRECO='VO')  E 
ON A.PRODUTO=E.PRODUTO 
JOIN (SELECT NOME_CLIFOR,CGC_CPF FROM CADASTRO_CLI_FOR) G 
ON B.MATRIZ=G.NOME_CLIFOR
JOIN (SELECT NOME_CLIFOR,CGC_CPF FROM CADASTRO_CLI_FOR) H  
ON A.FILIAL=H.NOME_CLIFOR
WHERE 
(B.MATRIZ IN ('ATACADO', 'GX NORTE FABRICA' ) OR A.FILIAL='ATACADO')      
AND A.SALDO>0     
ORDER BY B.MATRIZ ,A.FILIAL,A.PRODUTO   

ENDTEXT 

F_SELECT(XSEL,"CUR_INV_GX")

SELE CUR_INV_GX
COPY TO C:\TEMP\INV_GX_2012.XLS XLS