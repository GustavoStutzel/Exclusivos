text to xsel textmerge noshow

SELECT A.CODIGO_FILIAL,D.FILIAL,VALOR_TOTAL_COMPRAS_2013,C.* FROM
( SELECT CODIGO_CLIENTE,CODIGO_FILIAL,SUM(VALOR_PAGO) AS VALOR_TOTAL_COMPRAS_2013 FROM LOJA_VENDA WHERE YEAR(DATA_VENDA) = 2013 GROUP BY CODIGO_CLIENTE,CODIGO_FILIAL HAVING SUM(VALOR_PAGO)>0 ) A
LEFT JOIN 
( SELECT CODIGO_CLIENTE,CODIGO_FILIAL FROM LOJA_VENDA WHERE YEAR(DATA_VENDA) = 2014 GROUP BY CODIGO_CLIENTE,CODIGO_FILIAL HAVING SUM(VALOR_PAGO)>0  ) B
ON A.CODIGO_CLIENTE = B.CODIGO_CLIENTE


JOIN CLIENTES_VAREJO C
ON A.CODIGO_CLIENTE = C.CODIGO_CLIENTE


JOIN LOJAS_VAREJO D
ON A.CODIGO_FILIAL = D.CODIGO_FILIAL


WHERE B.CODIGO_CLIENTE IS NULL
AND D.FILIAL NOT LIKE '%ECOM%'


ORDER BY A.CODIGO_FILIAL asc ,VALOR_TOTAL_COMPRAS_2013 desc

endtext


f_select(xsel,"cur_exporta")

sele cur_exporta
copy to "c:\temp\clientes.xls" xls