SELECT B.MATRIZ,E.CGC_CPF AS CNPJ_MATRIZ,A.FILIAL,D.CGC_CPF AS CNPJ_FILIAL,
A.PRODUTO,C.DESC_PRODUTO,C.CLASSIF_FISCAL,A.SALDO,
ISNULL(F.PRECO1,0) AS PRECO_CUSTO,
ISNULL(G.PRECO1,0) AS PRECO_VAREJO_ORIGINAL
FROM (SELECT FILIAL,PRODUTO,SUM(SALDO) AS SALDO FROM TMP_SALDO_MARCO GROUP BY FILIAL,PRODUTO) A 
JOIN FILIAIS B 
ON A.FILIAL=B.FILIAL 
JOIN PRODUTOS C 
ON A.PRODUTO=C.PRODUTO 
JOIN CADASTRO_CLI_FOR D 
ON B.FILIAL=D.NOME_CLIFOR 
JOIN CADASTRO_CLI_FOR E  
ON B.MATRIZ=E.NOME_CLIFOR 
LEFT JOIN (SELECT PRODUTO,PRECO1 FROM PRODUTOS_PRECOS WHERE CODIGO_TAB_PRECO='CT') AS F 
ON A.PRODUTO=F.PRODUTO
LEFT JOIN (SELECT PRODUTO,PRECO1 FROM PRODUTOS_PRECOS WHERE CODIGO_TAB_PRECO='09') AS G 
ON A.PRODUTO=G.PRODUTO
WHERE A.SALDO<>0 
ORDER BY B.MATRIZ,B.FILIAL,A.PRODUTO


endtext 


