DEFINE CLASS bt_repr AS botao
	Enabled = .t.
	Visible = .t.
	autosize = .T.
	caption = 'Reprocessa'
	
	PROCEDURE click
		
		dodefault()
		
		IF thisformset.p_tool_status='P'
			IF !f_vazio(v_ctb_lancamento_01.lancamento) AND RECCOUNT('v_ctb_lancamento_01_item') = 0
				If !f_execute("EXECUTE LX_CTB_BORDERO_RECEBER_RETORNO ?v_ctb_lancamento_01.empresa, ?v_ctb_lancamento_01.lancamento")
					Return .F.
				ELSE
					thisformset.l_refresh()
				ENDIF
			ENDIF
		ENDIF
	ENDPROC
ENDDEFINE
